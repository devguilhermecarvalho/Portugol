programa{
funcao inicio()
{
	inteiro numero1, numero2, resultado
		
	escreva("Escreva o número da tabuada que deseja saber: ")
	leia(numero1)
		
	escreva("Escreva o segundo número da tabuada: ")
	leia(numero2)

	resultado = numero1 * numero2
		
	escreva("\nResultado: \n")
	escreva(numero1, " * ", numero2, " = ", resultado)
}}
